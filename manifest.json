{
    "name": "kibaco-advance",
    "author": "@aya172957",
    "description": "課題提出時にPDFのプレビューを表示させる拡張機能",
    "version": "1.1.1",
    "manifest_version": 3,
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://kibaco.tmu.ac.jp/portal/*"
            ],
            "js": [
                "preview.js",
                "notification.js",
                "timetable.js"
            ]
        },
        {
            "matches": [
                "https://kibaco.tmu.ac.jp/portal/*"
            ],
            "css": [
                "preview.css",
                "timetablestyle.css"
            ]
        }
    ]
  }